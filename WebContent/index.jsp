<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<%@ taglib prefix="s" uri="/struts-tags"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>HCMUS</title>
<style type="text/css">
#loginBox {
	border: 1px;
	border-color: black;
	background-color: #0066CC;
	height: 500px;
}

.logo {
	height: 200px;
	width: 200px;
	border-radius: 50%;
	background-color: #0066CC;
}

#btnLogin {
	width: 100%;
}

.container {
	display: grid;
	grid-template-columns: 25% 50% 25%;
	background-color: #2196F3;
	padding: 10px;
	background-color: #CCCCCC;
}

#lbUsername, #lbPassword {
	
}
</style>
</head>
<body>

	<h1>University Of Science</h1>

	<div class="container">
		<div></div>
		
		<center id="loginBox">
			<br> <br> <img class="logo" alt=""
				src="data:image/png;base64,">
			<br> <br>
			<s:form action="login" method="POST">
				<s:label id = "lbUsername" value="Username"></s:label>
				<s:textfield name="username"></s:textfield>
				<s:label id = "lbPassword" value="Password"></s:label>
				<s:password name="password"></s:password>
				<s:submit id="btnLogin" value="Login"></s:submit>
			</s:form>
			<s:a href="/ManageStudent/register.jsp">Register</s:a>
		</center>

	</div>

</body>
</html>